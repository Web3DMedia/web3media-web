{
  "name": "wigxel-nextjs-boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint && echo 'Lint complete.'",
    "watch": "watch 'clear && npm run -s lint' pages components",
    "prepare": "husky install"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "next": "^12.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "styled-components": "^5.0.1",
    "tailwindcss": "^3.0.13"
  },
  "devDependencies": {
    "@svgr/webpack": "^6.0.0",
    "@types/node": "^17.0.23",
    "@types/react": "^17.0.38",
    "autoprefixer": "^10.4.2",
    "eslint": "7",
    "eslint-config-next": "11.1.2",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^2.3.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.2",
    "postcss": "^8.4.5",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.5.1",
    "prettier-plugin-tailwindcss": "^0.1.4",
    "typescript": "^4.4.4",
    "watch": "^0.13.0"
  },
  "keywords": [
    "wigxel",
    "boilerplate",
    "nextjs"
  ],
  "lint-staged": {
    "*/**": "prettier --write --ignore-unknown",
    "*.js": "eslint --cache --fix",
    "*.ts": "npm run -s lint"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
